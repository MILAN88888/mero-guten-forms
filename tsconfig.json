{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"jsx": "react",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": false,
		"allowSyntheticDefaultImports": true
	},
	"include": [
		"src/**/*"
	]
}
